#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/ 
*betty style doc for
*
*function main goes the 
*/
int main(void)
{
int n;

srand(time(0));
n = rand() - RAND_MAX / 2;

printf("%d is\n", n);

if (n > 0)
printf("positive");

else if (n == 0)
printf("zero");

else (n < 0)
printf("negative");


return (0);
}
